<div class="row">
    <div class="col s12">
        <ul class="tabs">
            <% infos.forEach(function(data){
          var city_name_word = data.city.split(" ")[0]
          %>
                <li class="tab col s2">
                    <a href="#<%= city_name_word %>">
                        <%= data.city %>
                    </a>
                </li>
                <% }) %>
        </ul>
    </div>

    <% infos.forEach(function(datainfo){
    var city_name_word = datainfo.city.split(" ")[0]
    var country_name_word = datainfo.country.split(" ")[0]
    %>
        <div id="<%= city_name_word %>" class="col s12">
            <%= datainfo.country %>
                <div class="row">
                    <div class="col s12">
                        <ul class="tabs">
                            <li class="tab col s3">
                                <a href="#<%= country_name_word %>rest">
                                    <%= datainfo.city %> Restaurants</a>
                            </li>
                            <li class="tab col s3">
                                <a href="#<%= country_name_word %>att">
                                    <%= datainfo.city %> Attractions</a>
                            </li>
                        </ul>
                    </div>
                    <div id="<%= country_name_word %>rest" class="col s12">
                        <% locals.forEach(function(data1) { %>
                            <% data1.results.forEach(function(data){ %>
                                <% if (data.photos !== undefined && data.formatted_address.includes(datainfo.city)){ %>
                                    <%= data.formatted_address %>
                                        <img src = https://maps.googleapis.com/maps/api/place/photo?maxwidth=400&photoreference=<%= data.photos[0].photo_reference %>&key=AIzaSyAi5L8HFipThJzDSh6bghqzZD6NWp_pDh4 >
                                        <% }else if (data.photos == undefined && data.formatted_address.includes(datainfo.city)){ %>
                                            <b><%= data.name %></b>
                                            <%= data %>
                                                <% }}) %>
                                                    <% }) %>
                    </div>
                    <div id="<%= country_name_word %>att" class="col s12">Test 2</div>
                </div>
        </div>
        <% }) %>
</div>

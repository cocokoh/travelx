<div class= 'container'>
<div class="row">
    <form action="/register" method="POST" class="col s12">
      <div class="row">
        <div class="input-field col s6">
            <label for="first_name"></label>
          <input placeholder="First Name"id="first_name" name="first_name"type="text" class="validate">
        </div>
        <div class="input-field col s6">
          <label for="password"></label>
          <input placeholder="Password"id="password" name="password"type="password" class="validate">
        </div>
      </div>
      <div class="row">
        <div class="input-field col s6">
          <label for="contact"></label>
          <input placeholder="Contact"id="contact" name="contact"type="number" class="validate">
        </div>
        <div class="input-field col s6">
          <label for="authEmail"></label>
          <input placeholder="Email"id="authEmail" name="email" type="email" class="validate">
        </div>
      </div>
        <div class="input-field col s12">
          <label for="address"></label>
          <input placeholder="Address"id="address" name="address" type="text" class="validate">
        </div>

      <input type="submit" class="waves-effect waves-light btn" />
  </form>
</div>
</div>
